<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="my_addon_action" model="ir.actions.act_window">
        <field name="name">My Addon</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">My Addon</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="my_addon_view_tree" model="ir.ui.view">
        <field name="name">my.addon.view.tree</field>
        <field name="model">my.addon</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="my_addon_view_form" model="ir.ui.view">
        <field name="name">my_addon.view.form</field>
        <field name="model">my.addon</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>

